#include <iostream>
#include <string>
using namespace std;
class Barang
{
private:
string kodeBarang;
string namaBarang;

public:
    Barang(string kode, string nama)
    {
        kodeBarang = kode;
        namaBarang = nama;
    }

    void tampilkanInformasi()
    {
        cout << "Kode Barang: " << kodeBarang << endl;
        cout << "Nama Barang: " << namaBarang << endl;
    }
};

int main()
{
    Barang barang1("Kardus", "B.055");

    barang1.tampilkanInformasi();
    return 0;
}